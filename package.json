{
  "name": "ToDo",
  "version": "1.0.0",
  "description": "React TO-DO application",
  "main": "''",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --collectCoverage",
    "start": "webpack-dev-server --open --config ./config/webpack.config.dev.js",
    "lint": "eslint --ext .js --ext .jsx --fix src config __tests__",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch",
    "lint:styles": "stylelint --fix --syntax scss **/*.scss",
    "clean-public": "npm run remove-public && mkdir public",
    "remove-public": "node_modules/.bin/rimraf ./public",
    "prebuild": "npm-run-all clean-public lint",
    "build": "webpack --config ./config/webpack.config.prod.js",
    "format": "prettier \"src/**/*.{js,jsx,yml,yaml,ts,tsx,md,graphql,mdx}\" --write",
    "format:styles": "prettier \"src/**/*.{scss,sass,less,css}\" --parser scss --write",
    "precommit": "lint-staged",
    "predeploy": "npm run build",
    "deploy": "now",
    "prod:now": "now --prod",
    "dev:now": "now dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ArturW1998/ToDo"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "webpack"
  ],
  "author": "Artur Voloshyn (https://github.com/ArturW1998)",
  "license": "MIT",
  "homepage": "https://github.com/ArturW1998/ToDo",
  "bugs": {
    "url": "https://github.com/ArturW1998/ToDo/issues"
  },
