{
  "name": "react-todo-app",
  "version": "1.0.0",
  "description": "React TO-DO application",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --collectCoverage",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "webpack-dev-server --open --config ./webpack.config.dev.js",
    "lint": "node_modules/.bin/esw webpack.config.* src",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch",
    "lint:styles": "stylelint --fix --syntax scss **/*.scss",
    "clean-public": "npm run remove-public && mkdir public",
    "remove-public": "node_modules/.bin/rimraf ./public",
    "prebuild": "webpack --config ./webpack.config.prod.js",
    "build": "npm-run-all clean-public lint prebuild",
    "format": "prettier \"src/**/*.{js,jsx,yml,yaml,ts,tsx,md,graphql,mdx}\" --write",
    "format:styles": "prettier \"src/**/*.{scss,sass,less,css}\" --parser scss --write",
    "precommit": "lint-staged",
    "predeploy": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ArturW1998/ToDo"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "webpack"
  ],
  "author": "Artur Voloshyn (https://github.com/ArturW1998)",
  "license": "MIT",
  "homepage": "https://github.com/ArturW1998/ToDo",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "cheerio": "^0.20.0",
    "colors": "1.1.2",
    "compression": "^1.6.1",
    "open": "0.0.5",
    "react": "^15.3.1",
    "react-dom": "^15.0.1",
    "react-favicon": "0.0.4",
    "react-redux": "^4.4.5",
    "react-redux-toastr": "^6.2.7",
    "react-router": "^2.7.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "serve-favicon": "^2.3.0",
    "express": "4.13.4",
    "react-hot-loader": "^4.8.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/parser": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
